<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mappers.user.products.GetProductMapper">


<select id="selectPdCateFromPdSeq" parameterType="int" resultMap="com.nemo.user.products.repository.impl.UserProductsCategoryMapper.productsCateMap">
	SELECT b.*
	FROM product_tb a
	INNER JOIN product_cate_tb b
	ON a.product_cate_no = b.product_cate_no and a.product_no = ${productNo};
</select>

</mapper>